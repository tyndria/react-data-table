  "description": "See https://tyndria.github.io/react-data-table",
  "private": true,
  "homepage": "https://tyndria.github.io/react-data-table",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "gh-pages": "^1.0.0",
    "node-sass-chokidar": "0.0.3",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.10",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
